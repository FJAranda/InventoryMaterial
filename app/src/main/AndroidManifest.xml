<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.inventoryMVP">

    <application
        android:name="com.example.inventoryMVP.InventoryApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_icono"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:node="replace">
        <activity android:name="com.example.inventoryMVP.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.inventoryMVP.ui.login.LoginViewImplement" />
        <activity android:name="com.example.inventoryMVP.InventoryActivity" />
        <activity android:name="com.example.inventoryMVP.DashboardActivity" />
        <activity android:name="com.example.inventoryMVP.ProductActivity" />
        <activity android:name="com.example.inventoryMVP.ui.dependency.DependencyActivity" />
        <activity android:name="com.example.inventoryMVP.SectorActivity" />
        <activity android:name="com.example.inventoryMVP.AccountSettingsActivity" />
        <activity android:name="com.example.inventoryMVP.GeneralSettingsActivity" />
        <activity android:name="com.example.inventoryMVP.ui.about.AboutActivity" />
    </application>

</manifest>